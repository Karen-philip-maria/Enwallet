                app:layout_constraintEnd_toStartOf="@+id/expenses"
                app:layout_constraintTop_toBottomOf="@+id/moneyOut" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
